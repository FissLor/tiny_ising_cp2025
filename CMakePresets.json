{
    "version": 3,
    "configurePresets": [

        {
            "name": "gcc-default",
            "displayName": "Compile using gcc with -O3",
            "description": "Uses Ninja generator, -Wall and -Wextra flags are used",
            "generator": "Ninja",
            "cacheVariables": {
                "CMAKE_BUILD_TYPE": "Debug",
                "CMAKE_C_FLAGS": "-DXOSHIRO256PP -DZERO_PADDING -O3 -Wall -Wextra",
                "CMAKE_C_STANDARD": "11",
                "CMAKE_C_COMPILER": "gcc",
                "CMAKE_INSTALL_PREFIX": "${sourceDir}/out/${presetName}/install/"
            }
        },
        {
            "name": "gcc-O3",
